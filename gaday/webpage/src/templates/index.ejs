<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>
        <%= htmlWebpackPlugin.options.title %>
    </title>
    <!-- <%= require('ejs-loader!./_meta.ejs')({ data: htmlWebpackPlugin.options }) %> -->
    <!-- <%= require('ejs-loader!./_favicons.ejs')() %> -->
    <%= require('ejs-loader!./pre-load.ejs')() %>
</head>

<body>
  <script type="text/javascript">
  	window.ContributorsData = JSON.parse('<%= JSON.stringify(htmlWebpackPlugin.options.contributors) %>')
    window.onload = function() {
      setTimeout(()=>{
        document.getElementById('load').setAttribute('style', 'display: none;');
        document.getElementById('page').setAttribute('style', 'display: block;');
      },600);
      // document.getElementById('load').setAttribute('style', 'display: none;');
      // document.getElementById('page').setAttribute('style', 'display: block;');
    };
  </script>
  <%= require('ejs-loader!./_browserhappy.ejs')() %>
        
  <div id="load" class="pre-load">loading...</div>

  <div id="page" style="display: none;">
      <div id="mask" class="modal-mask" style="display: none;">
        <button type="button" id="btn-close" class="btn-close" onclick="closeMask()"><i class="icon"></i></button>  
        <%= require('ejs-loader!./starwall.ejs')() %>
        <%= require('ejs-loader!./authform.ejs')() %>
      </div>

      <%= require('ejs-loader!./gaday.ejs')() %>
  </div>
  
  <!-- <%= require('ejs-loader!./_version.ejs')({ data: htmlWebpackPlugin.options }) %> -->
  <%= require('ejs-loader!./_analytics.ejs')({ data: htmlWebpackPlugin.options }) %>
</body>

</html>
